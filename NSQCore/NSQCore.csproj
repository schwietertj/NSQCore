<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.1</TargetFramework>
    <Title>NSQCore - .NET Core Client for NSQ</Title>
	<PackageId>NSQCore</PackageId>
    <PackageVersion>1.0.0</PackageVersion>
    <Authors>Kailesh</Authors>
	<Owners>Webjet</Owners>
    <Description>.Net Core client for NSQ </Description>
    <PackageRequireLicenseAcceptance>false</PackageRequireLicenseAcceptance>
    <PackageReleaseNotes>First release</PackageReleaseNotes>
    <Copyright>Copyright 2017 (c) Webjet Ltd. All rights reserved.</Copyright>
    <PackageTags>NSQ NSQCore .NetCore</PackageTags>
	<PackageProjectUrl>https://github.com/Webjet/NSQCore</PackageProjectUrl>
	<PackageLicenseUrl>https://github.com/Webjet/NSQCore/blob/master/LICENSE.md</PackageLicenseUrl>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.Extensions.Logging" Version="3.0.0" />
    <PackageReference Include="Microsoft.Extensions.Logging.Abstractions" Version="3.0.0" />
    <PackageReference Include="Microsoft.Extensions.Logging.Console" Version="3.0.0" />
    <PackageReference Include="Newtonsoft.Json" Version="12.0.2" />
  </ItemGroup>

</Project>
